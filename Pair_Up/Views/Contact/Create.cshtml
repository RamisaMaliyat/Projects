@model First_Checkpoint.Models.Contact
@{

}
<!DOCTYPE html>
<html>
<head>
    <title>ContactUs</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Client Contact Form template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free web designs for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Custom Theme files -->
    <link href="~/ContactPage/css/StyleSheet.css" rel="stylesheet" type="text/css" media="all" />
    <!-- //Custom Theme files -->
    <!-- web font -->
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'><!--web font-->
    <link href="//fonts.googleapis.com/css?family=Amita:400,700" rel="stylesheet">
    <!-- //web font -->
</head>
<body>
    <!-- main -->
    <div class="container">
        <div class="main">
            <div></div>
            <h1>Contact Us</h1>



            <div class="col-md-6">
                <div class="main">
                    <div class="container">
                        <div class="box">
                            <div class="icon">
                                <i class="fa fa-map-marker" aria-hidden="true"></i>

                            </div>
                            <div class="text">
                                <h3><img src="~/ContactPage/images/location.png" /> Address</h3>
                                <p>141 & 142, Love Road, Tejgaon Industrial Area<br>Dhaka 1208 Dhaka Bangladesh</p>
                            </div>
                        </div>
                        <div class="box">

                            <div class="text">
                                <h3> <img src="~/ContactPage/images/phone.png" alt=""> Phone</h3>
                                <p> +880 (2) 887 0422</p>
                                <p>+880 (2) 887 0417</p>
                            </div>
                        </div>
                        <div class="box">
                            <div class="icon">

                            </div>
                            <div class="text">
                                <h3><img src="~/ContactPage/images/mail.png" /> Email</h3>
                                <p>180104079@aust.edu</p>
                                <p>180104094@aust.edu</p>
                                <p>170204083@aust.edu</p>
                                <p></p>
                            </div>
                        </div>
                    </div>



                </div>
                <fieldset>
                    <h3>Find Location...</h3>
                    <iframe src="https://maps.google.com/maps?q=AhsanullahUniversityofScienceandTechnology%20Dates%20Products&amp;t=&amp;z=13&amp;ie=UTF8&amp;iwloc=&amp;output=embed"></iframe>
                </fieldset>
            </div>





            <div class="col-md-6">
                <div class="main-row">

                    <div class="agileits-info">
                        <div class="agileits-infoleft">
                            @*  <img src="~/ContactPage/images/img1.jpg" alt="" />*@
                        </div>
                        @*  <div class="agileits-inforight">
                                <h2>Jose Leiter</h2>
                                <h6>Fashion Designer</h6>
                                <p><img src="~/ContactPage/images/i1.png" alt=""> +01 11 222 3333</p>
                                <p><img src="~/ContactPage/images/i2.png" alt=""> <a href="mailto:example@mail.com"> mail@example.com</a></p>
                            </div>*@
                        <div class="clear"> </div>
                    </div>
                    <center>
                        <h3>Contact Us...</h3>
                        <h3>SEND A QUICK MESSAGE</h3>
                    </center>
                    <div class="contact-wthree">
                        <center>
                            @using (Html.BeginForm("Create", "Contact", FormMethod.Post))

                            {
                                @Html.AntiForgeryToken()
                                @Html.TextBoxFor(m => m.FullName, new { @placeholder = "Full name" })
                                @Html.ValidationMessageFor(m => m.FullName)
                                @Html.TextBoxFor(m => m.Email, new { @placeholder = "Email" })
                                @Html.ValidationMessageFor(m => m.Email)
                                @Html.TextBoxFor(m => m.PhoneNumber, new { @placeholder = "PhoneNumber" })
                                @Html.ValidationMessageFor(m => m.PhoneNumber)
                                @Html.TextAreaFor(m => m.Message, new { @placeholder = "Message" })
                                @Html.ValidationMessageFor(m => m.Message)
                                <input type="submit" value="Sumbit" />
                            }
                        </center>

                    </div>

                </div>
            </div>





        </div>
    </div>
    <!-- //main -->
    <!-- copyright
    <div class="w3copyright-agile">
        <p>© 2017 Client Contact Form. All rights reserved | Design by <a href="http://w3layouts.com/" target="_blank">W3layouts</a></p>
    </div>
    <!-- //copyright -->




</body>




</html>





